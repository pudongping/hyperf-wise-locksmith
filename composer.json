    "keywords": [
        "lock",
        "locking",
        "mutex",
        "serialized",
        "redlock",
        "hyperf"
    ],
    "homepage": "https://github.com/pudongping/hyperf-wise-locksmith",
    "license": "MIT",
    "authors": [
        {
            "name": "pudongping",
            "email": "pudongping67@gmail.com",
            "homepage": "https://github.com/pudongping"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pudongping\\HyperfWiseLocksmith\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "require": {
        "php": ">=8.1",
        "hyperf/logger": "~3.1.0",
        "hyperf/redis": "~3.1.0",
        "pudongping/wise-locksmith": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
